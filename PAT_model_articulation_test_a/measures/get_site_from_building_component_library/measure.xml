      <name>zipcode</name>
      <display_name>Zip Code for project</display_name>
      <description>Enter valid us 8 digit zipcode</description>
      <type>Integer</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>80401</default_value>
    </argument>
